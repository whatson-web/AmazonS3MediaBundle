<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Backend Template - Whatson Web</title>
    <meta name="robots" content="noindex, nofollow">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

    <![endif]-->

    {% stylesheets
    '@WHBackendTemplateBundle/Resources/public/plugins/bootstrap/css/bootstrap.min.css'
    '@WHAmazonS3MediaBundle/Resources/public/css/dropzone.css'
    filter="cssrewrite" %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css"/>

    {% stylesheets
    '@WHBackendTemplateBundle/Resources/public/plugins/backend-template/css/layout.scss'
    '@WHAmazonS3MediaBundle/Resources/public/css/layout.scss'
    filter="scssphp" %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}

</head>

<body>

{% import '@WHBackendTemplate/Twig/macros.html.twig' as macros %}

<div class="container">
    {% block body %}{% endblock %}
</div>

{% javascripts
'@WHBackendTemplateBundle/Resources/public/plugins/jquery/js/jquery.min.js'
'@WHBackendTemplateBundle/Resources/public/plugins/jquery-ui/js/jquery-ui.min.js'
'@WHBackendTemplateBundle/Resources/public/plugins/bootstrap/js/bootstrap.min.js'
'@WHAmazonS3MediaBundle/Resources/public/js/dropzone.js'
'@WHBackendTemplateBundle/Resources/public/plugins/backend-template/js/layout.js' %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}

{% block javascript %}{% endblock %}

</body>
</html>